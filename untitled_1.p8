--gentils

function gentil(x,y)
	if not check_flag(0,x/8,y/8) then
		toto=x
		tata=y
		else
		toto=aleatoire(33*8,46*8)
		tata=aleatoire(8*8,15*8)
	end
	
	gentil1={
				--x=aleatoire(36,46)*8,
				--y=aleatoire(0,15)*8,
		x=toto,
		y=tata,
		sprite=aleatoire(49,53),

		}
	add(gentils,gentil1)
	


end

--aleatoire entre a et b inclus
function aleatoire(a,b)
	return flr(rnd(b+1-a)+a)
end

	


