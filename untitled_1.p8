	init_msg()
	gentils={}
	score=0
	state=0
	
end


function _update()
 check_flag(flag,x,y)
 update_camera()
 update_enemies()
 update_enemies_1()
 update_bullets()
 player_movement()
 update_msg()

	if #enemies==0 then
 	spawn_enemies(ceil(rnd(4)))
 end
 if #enemies_1==0 then
 	spawn_enemies_1(ceil(rnd(5)))
 end
 if btnp(‚ùé) then
 	shoot()
 end
 update_recommencer()
end



function _draw()
