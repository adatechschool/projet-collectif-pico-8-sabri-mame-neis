--balles

function shoot()
	new_bullet={
		x=p.x*8,
		y=p.y*8,
		speed=4,
	}
	add(bullets,new_bullet)
end

function update_bullets()
	for b in all(bullets) do
		b.y-=b.speed
	end
end



