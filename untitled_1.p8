--cles

function next_tile(x,y)
 sprite=mget(x,y)
 mset(x,y,sprite+1)
end

function pick_up_key(x,y)
 next_tile(x,y)
 p.keys+=1
end

function open_door(x,y)
 next_tile(x,y)
 p.keys-=1
end
